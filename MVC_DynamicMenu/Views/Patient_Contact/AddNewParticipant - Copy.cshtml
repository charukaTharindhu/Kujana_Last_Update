
@model MVC_DynamicMenu.Models.Patient
@{
    ViewData["Title"] = "Home page";
}
<head>
    <link href="~/css/Addfile.css" rel="stylesheet" />
</head>
<div class="container">
    <header class="header">
        <h1 id="title" class="text-center">New Participant</h1>
        <p id="description" class="description text-center">

        </p>
    </header>

    @*Demographic*@
    <form asp-action="AddNewParticipant" id="survey-form">
        <div class="form-group">
            <p>Demographic</p>
            <label id="name-label" asp-for="@Model.Patient_Info[0].NDIS_Number">NDIS Number</label>
            <input type="text"
                   asp-for="@Model.Patient_Info[0].NDIS_Number"
                   id="name"
                   class="form-control"
                   placeholder="--9 characters starts with 43--"
                   required />




            <label id="name-label" asp-for="@Model.Patient_Info[0].LastName">Last Name</label>
            <input type="text"
                   asp-for="@Model.Patient_Info[0].LastName"
                   id="name"
                   class="form-control"
                   required />

            <h6>Gender</h6>
            <select id="dropdown" asp-for="@Model.Patient_Info[0].Gender" class="form-control" required>
                @*<option disabled selected value>Gendere</option>*@
                <option value="student">Male</option>
                <option value="job">Femail</option>
                <option value="learner">Other</option>
            </select>

            <label asp-for="@Model.Patient_Info[0].DateOfBirth">Date Of Birth</label>
            <input type="date" asp-for="@Model.Patient_Info[0].DateOfBirth" id="dateofbirth">

            <h6>Office</h6>
            <select id="dropdown" asp-for="@Model.Patient_Info[0].Office" class="form-control" required>
                @*<option disabled selected value>Gendere</option>*@
                <option value="student">1</option>
                <option value="job">2</option>
                <option value="learner">3</option>
            </select>


        </div>


        <script type="text/javascript">
            const projectName = 'survey-form';

        </script>
    </form>
</div>






